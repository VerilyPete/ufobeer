{
	"name": "ufobeer",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "vitest",
		"test:unit": "vitest --config vitest.unit.config.mts",
		"typecheck": "tsc --noEmit",
		"typecheck:test": "tsc --project test/tsconfig.json --noEmit",
		"migrate:remote": "wrangler d1 migrations apply beer-db --remote",
		"migrate:local": "wrangler d1 migrations apply beer-db --local",
		"migrate:list": "wrangler d1 migrations list beer-db --remote",
		"cf-typegen": "wrangler types"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.69.0"
	},
	"dependencies": {
		"p-limit": "^5.0.0",
		"zod": "^3.25.76"
	}
}
